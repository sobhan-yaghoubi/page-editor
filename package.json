{
  "name": "@sobhan.yaghoubi/page-editor",
  "version": "0.1.2",
  "description": "A powerful, schema-driven page-editor for React and Next.js.",
  "author": "sobhan yaghoubi",
  "license": "ISC",
  "private": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./editor": {
      "import": {
        "types": "./dist/editor.d.ts",
        "default": "./dist/editor.mjs"
      },
      "require": {
        "types": "./dist/editor.d.ts",
        "default": "./dist/editor.cjs"
      }
    },
    "./view": {
      "import": {
        "types": "./dist/view.d.ts",
        "default": "./dist/view.mjs"
      },
      "require": {
        "types": "./dist/view.d.ts",
        "default": "./dist/view.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sobhan-yaghoubi/page-editor.git"
  },
  "keywords": [
    "react",
    "nextjs",
    "page-builder",
    "editor",
    "headless",
    "e-commerce"
  ],
  "peerDependencies": {
    "clsx": "^2.1.1",
    "lodash": "^4.17.21",
    "lucide-react": ">=0.300.0",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0",
    "zustand": ">=4.5.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.0",
    "@types/react": "^18.2.0",
    "@types/uuid": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "uuid": "^13.0.0"
  }
}
